<?xml version="1.0" encoding="utf-8" ?>
<configuration>
	<configSections>
		<section name="hibernate-configuration" type="NHibernate.Cfg.ConfigurationSectionHandler, NHibernate" />
		<!-- For more information on Entity Framework configuration, visit http://go.microsoft.com/fwlink/?LinkID=237468 -->
		<section name="entityFramework" type="System.Data.Entity.Internal.ConfigFile.EntityFrameworkSection, EntityFramework, Version=6.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" requirePermission="false" />
		<sectionGroup name="spring">
			<section name="context" type="Spring.Context.Support.ContextHandler, Spring.Core" />
			<section name="objects" type="Spring.Context.Support.DefaultSectionHandler, Spring.Core" />
		</sectionGroup>
	</configSections>
	<appSettings>
		<!--Conexão de banco de dados SQL Data Client-->
		<add key="connectionString" value="Server=localhost;Database=db_teste;Uid=user_teste;Pwd=12345678" />
	</appSettings>
	<spring>
		<context>
			<resource uri="config://spring/objects" />
		</context>
		<objects xmlns="http://www.springframework.net">
			<!--Connections-->
			<object name="ConnectionFactoryDAO" type="AppClassLibraryDomain.DAO.SQL.ConnectionFactoryDAO, AppClassLibraryDomain" />
			<object id="ConnectionFactory" type="System.String" factory-method="Copy">
				<constructor-arg index="0">
					<value>Data Source=localhost;Initial Catalog=db_teste;User ID=user_teste;Password=12345678;Max Pool Size=10000;Connect Timeout=1200000;Application Name=SisContatos</value>
				</constructor-arg>
			</object>
			<!--DAOs-->
			<object name="ContatoSQLDAO" type="AppClassLibraryDomain.DAO.SQL.ContatoDAO, AppClassLibraryDomain">
				<property name="ConnectionFactoryDAO" ref="ConnectionFactoryDAO" />
			</object>
			<object name="UsuarioSQLDAO" type="AppClassLibraryDomain.DAO.SQL.UsuarioSQLDAO, AppClassLibraryDomain">
				<property name="ConnectionFactoryDAO" ref="ConnectionFactory"/>
			</object>
			<!--Services-->
			<object name="ContatoService" type="AppClassLibraryDomain.service.ContatoService, AppClassLibraryDomain" >
				<property name="ContatoDAO" ref="ContatoSQLDAO" />
			</object>
			<object name="UsuarioService" type="AppClassLibraryDomain.service.UsuarioService, AppClassLibraryDomain" >
				<property name="UsuarioDAO" ref="UsuarioSQLDAO" />
			</object>
			<!--Pages-->
			<!--<object name="FormListarEmSQL" type="~/pages/FormListarEmSQL.aspx" >
				<property name="ContatoService" ref="ContatoService" />
			</object>-->
		</objects>
	</spring>
	<!--Conexão de banco de dados NHibernate-->
	<hibernate-configuration xmlns="urn:nhibernate-configuration-2.2">
		<session-factory>
			<property name="connection.provider">NHibernate.Connection.DriverConnectionProvider</property>
			<property name="connection.driver_class">NHibernate.Driver.SqlClientDriver</property>
			<property name="connection.connection_string">Server=localhost;Database=db_teste;User ID=user_teste;Password=12345678</property>
			<property name="dialect">NHibernate.Dialect.MsSql2008Dialect</property>
			<property name="show_sql">true</property>
		</session-factory>
	</hibernate-configuration>
	<entityFramework>
		<providers>
			<provider invariantName="System.Data.SqlClient" type="System.Data.Entity.SqlServer.SqlProviderServices, EntityFramework.SqlServer" />
		</providers>
	</entityFramework>
	<!--Conexão de banco de dados Entity Framework-->
	<connectionStrings>
		<add name="connection_string"
			  providerName="System.Data.SqlClient"
			  connectionString="Server=localhost;Database=db_teste;User ID=user_teste;Password=12345678"/>
	</connectionStrings>
</configuration>